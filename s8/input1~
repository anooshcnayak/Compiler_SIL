decl
	integer i, j, n, temp, a[100];
enddecl
integer main() {
begin
	read(n);
	i = 0;
	
	while(i < n) do
		read(a[i]);
		i = i+1;
	endwhile;
	
	i = 0;
	j = 0;
	
	while(i < (n-1)) do
		while(j < (n-i-1)) do
			if(a[j] < array[j+1]) then
				j = j+1;
			endif;
			j = j+1;
		endwhile;
		i = i+1;
	endwhile;
	
	i = 0;
	while(i < n) do
		write(a[i]);
		i = i+1;
	endwhile;
	
	return 1;
end
}
